{
  "name": "zenberry-api",
  "version": "1.0.0",
  "description": "E-commerce API with NestJS",
  "author": "Zenberry Team",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "copy:assets": "cp -r prisma dist && copyfiles -u 1 src/**/*.csv src/**/*.mo src/**/**/*.mo src/**/*.py src/**/**/*.py src/**/*.wasm src/**/**/*.wasm src/**/**/**/*.wasm src/**/**/**/**/*.wasm src/**/*.sol src/**/**/*.sol src/**/**/**/*.sol src/**/**/**/**/*.sol src/**/**/**/**/**/*.sol src/**/**/**/**/**/**/*.sol src/**/*.yaml src/**/**/*.yaml src/**/**/**/*.yaml src/**/**/*.wav src/**/**/**/*.wav src/**/*.wav dist/",
    "build": "rimraf dist && nest build -b swc && npm run copy:assets",
    "build:dev": "rimraf dist && node --max_old_space_size=4096 ./node_modules/@nestjs/cli/bin/nest.js build && xcopy /E /I prisma dist\\prisma",
    "prisma:generate:all": "npx prisma generate --schema=prisma/schema.prisma",
    "prisma:migrate:main": "npx prisma migrate deploy --schema=prisma/schema.prisma",
    "start:prod": "npm run prisma:generate:all && npm run prisma:migrate:main && node dist/main",
    "start:dev:build": "prisma generate --schema=dist/prisma/schema.prisma && node --max_old_space_size=6144 dist/main",
    "start": "nest start",
    "start:swc": "nest start -b swc",
    "start:dev": "set NODE_OPTIONS=--max-old-space-size=8192 && nest start --watch",
    "migrate:dev:local": "npx prisma migrate dev",
    "migrate:deploy:local": "npx prisma migrate deploy",
    "db:pull": "npx prisma db pull --schema=prisma/schema.prisma",
    "prisma:sync": "rm -rf node_modules/.prisma && npx prisma generate --schema=prisma/schema.prisma",
    "prisma:sync:windows": "rmdir /s /q node_modules\\.prisma 2>nul & npx prisma generate --schema=prisma/schema.prisma"
  },
  "dependencies": {
    "@langchain/community": "^1.0.7",
    "@langchain/core": "^1.1.4",
    "@langchain/google-genai": "^2.0.4",
    "@langchain/langgraph": "^1.0.4",
    "@nestjs/common": "^10.4.4",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.4.15",
    "@nestjs/platform-socket.io": "^10.4.19",
    "@nestjs/platform-ws": "^10.4.20",
    "@nestjs/schedule": "^4.1.2",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^10.4.20",
    "@prisma/client": "^4.9.0",
    "axios": "^1.3.3",
    "bcrypt": "^6.0.0",
    "body-parser": "^1.20.3",
    "cache-manager": "^5.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "crypto-js": "^4.2.0",
    "ioredis": "^5.4.1",
    "langchain": "^1.1.5",
    "multer": "^1.4.5-lts.1",
    "nestjs-cls": "^6.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.6.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.1.4",
    "@nestjs/testing": "^10.4.4",
    "@swc/cli": "^0.5.2",
    "@swc/core": "^1.3.64",
    "@swc/helpers": "^0.5.2",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.19.116",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^10.1.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "git-commit-msg-linter": "^4.5.0",
    "husky": "^8.0.3",
    "jest": "29.3.1",
    "prettier": "^2.3.2",
    "prisma": "^4.8.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^5.0.0"
  }
}
